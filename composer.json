{
    "name": "hatchetaustrlia/laravel-schedule-testing",
    "description": "A lightweight package for testing Laravel schedules.",
    "homepage": "https://github.com/hatchetaustralia/laravel-schedule-testing",
    "keywords": [
        "php",
        "laravel",
        "package",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Glenn Forrest",
            "email": "glennjamesforrest@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "nesbot/carbon": "^2.64"
    },
    "require-dev": {
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "orchestra/testbench": "^7.18",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Hatchet\\LaravelScheduleTesting\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test:styles": "phpcs",
        "test:types": "phpstan analyse --ansi",
        "test:unit": "phpunit --colors=always",
        "test": [
            "@test:styles",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hatchet\\LaravelScheduleTesting\\LaravelScheduleTestingServiceProvider"
            ]
        }
    }
}
